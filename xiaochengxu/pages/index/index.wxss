page{
    background: #c9dbe9;
}
/* we的名称 */
.woodengine-logo {
    font-family: -apple-system, blinkmacsystemfont, 'segoe ui', roboto, oxygen, ubuntu, cantarell,
    'open sans', 'helvetica neue', sans-serif;
    font-size: 100rpx;
    font-weight: lighter;
  }
.woodengine-logo::before {
    content: 'Wood';
    color:#9bd25d;
  }
.woodengine-logo::after {
    content: 'Engine';
    color:#004b87;
  }
  .dm-bottom {
    position:absolute;
    bottom:40rpx;
    right:50%;
    transform: translateX(50%);
    width:100%;
  }
  .dm-color-green {
    color:#9bd25d;
  }
  .dm-top-right {
    position: absolute;
    top: 15%;
    right: 30%;
  }


  /**用户样式**/
